    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.4.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "npm-run-all": "^4.1.5",
    "nuxt": "3.0.0-rc.9",
    "vue-eslint-parser": "^9.0.3",
    "vue-tsc": "^0.40.8"
  },
  "lint-staged": {
