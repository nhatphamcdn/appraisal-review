{
  "private": true,
  "engines": {
    "node": ">=16.14.0",
    "npm": ">=8.3.1"
  },
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "prepare": "husky install",
    "lint": "npm-run-all lint:*",
    "lint:types": "nuxi typecheck",
    "lint:style": "eslint .",
    "test": "npm-run-all postinstall && TZ=UTC vitest --run",
    "test:watch": "npm-run-all postinstall && TZ=UTC vitest",
    "test:ui": "npm-run-all postinstall && TZ=UTC vitest --ui",
    "story:dev": "histoire dev",
    "story:build": "histoire build",
    "story:preview": "histoire preview"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@histoire/plugin-nuxt": "^0.10.7",
    "@histoire/plugin-vue": "^0.10.7",
    "@nhatphamfks-org/funstyle": "^1.0.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/vue": "^6.6.1",
    "@types/node": "^18.7.15",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "@vitejs/plugin-vue": "^3.1.0",
    "@vitest/coverage-c8": "^0.23.1",
    "@vitest/ui": "^0.23.1",
    "@vue/eslint-config-typescript": "^11.0.0",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.4.0",
    "histoire": "^0.10.7",
    "husky": "^8.0.1",
    "jsdom": "^20.0.0",
    "lint-staged": "^13.0.3",
    "npm-run-all": "^4.1.5",
    "nuxt": "3.0.0-rc.9",
    "sass": "^1.54.8",
    "unplugin-auto-import": "^0.11.2",
    "vite-svg-loader": "^3.6.0",
    "vitest": "^0.23.1",
    "vue-eslint-parser": "^9.0.3",
    "vue-tsc": "^0.40.8"
  },
  "lint-staged": {
    "*.{js,ts,vue}": [
      "eslint --fix .",
      "prettier --write"
    ],
    "*.{scss, css, md}": [
      "prettier --write"
    ]
  }
}
